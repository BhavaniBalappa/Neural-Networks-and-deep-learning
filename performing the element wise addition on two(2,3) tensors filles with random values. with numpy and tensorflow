#USING NUMPY
import numpy as np

A = np.array([[1, 2, 3],
              [4, 5, 6]])

B = np.array([[7, 8, 9],
              [10, 11, 12]])

C = A + B

print(C)

#USING TENSORFLOW
import tensorflow as tf

tensor1 = tf.constant([[1, 2, 3],
                        [4, 5, 6]])

tensor2 = tf.constant([[7, 8, 9],
                        [10, 11, 12]])

tensor_sum = tf.add(tensor1, tensor2)

print(tensor_sum)


